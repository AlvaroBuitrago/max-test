<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>

<c74object name="test.terminate" module="max" category="Testing">
	<digest>End a test</digest>
	<description>
		<o>test.terminate</o> takes a <m>bang</m> to indicate that a test is complete and should be terminated. 
	</description>
	
	
	<!--METADATA-->
	<metadatalist>
		<metadata name="author">Cycling '74</metadata>
		<metadata name="tag">Testing</metadata>
	</metadatalist>
	

	<!--ARGUMENTS-->
	<objarglist/>
	

	<!--MESSAGES-->
	<methodlist>
		<method name="bang">
			<arglist />
			<digest>terminate a test</digest>
			<description>
				Note: DSP will be turned-off as a part of terminating the test.
			</description>
		</method>	
	</methodlist>
	
	
	<!--ATTRIBUTES-->


	<!--SEEALSO-->
	<seealsolist>
		<seealso name="test.assert" />
		<seealso name="test.log" />
		<seealso name="test.sample~" />
	</seealsolist>


</c74object>
